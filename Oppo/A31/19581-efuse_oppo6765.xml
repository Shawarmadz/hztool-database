<?xml version="1.0" encoding="UTF-8" ?>
<flashtool-config version="2.0">
	<general>
		<chip-name>MT6765</chip-name>
		<storage-type>EMMC</storage-type>
		<download-agent>
			MTK_AllInOne_DA.bin
		</download-agent>
		<scatter>
			MT6765_Android_scatter.txt
		</scatter>
		<rom-list>
			<rom index="0" enable="true">
				preloader_oppo6765_19581.bin
			</rom>
		</rom-list>
		<connection
				type="BromUSB"
				high-speed="false"
				without-battery="false" />
	</general>
	<commands>
		<efuse>
			<common-ctrl
			        Disable_NAND_boot_speedup="false"
		   			Disable_UFS_boot="false"			
					USB_download_type="0"
					Disable_NAND_boot="false"
					Disable_EMMC_boot="false"/>

			<secure-ctrl
					Disable_DBGPORT_LOCK="false"
					Enable_SW_JTAG_CON="false"
					Enable_ACC="false"
					Enable_ACK="false"
					Enable_SLA="false"
					Enable_DAA="true"
					Enable_SBC="true"
					Disable_JTAG="false"/>

			<sbc-pub-key>
				<key-type>pss</key-type>
				<pub-key-e>010001</pub-key-e>
				<pub-key-n>
                   
				</pub-key-n>
			</sbc-pub-key>

			<common-lock
					com_ctrl_lock="false"
					usb_id_lock="false"/>

			<secure-lock
					custk_lock = "false"
					sec_msc_lock = "false"
					sec_attr_lock="false"
					ackey_lock="false"
					sbc_pubk_hash_lock="false"/>

			<usb-id vid="0000" pid="0000"/> 

			<sec_msc
					md1_sbc_en="false"
					c2k_sbc_en="false" />

			<c_lock
					c_data5_lock="false"
					c_data4_lock="false"
					c_data3_lock="false"
					c_data2_lock="false"
					c_data1_lock="false"
					c_data0_lock="false"
					c_ctrl1_lock="false" 
					c_ctrl0_lock="false"
					c_ctrlm_lock="false"/>


